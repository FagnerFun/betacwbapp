<?xml version="1.0" encoding="UTF-8" ?>
<resources>
	<style name="linearPrincipal">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>    	
        <item name="android:background">#F5F5F5</item>
	</style>
	<style name="linearPrincipal2">
		<item name="android:orientation">vertical</item>
		<item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">fill_parent</item>    	
    <item name="android:background">#FFF46F</item>
	</style>
	<style name="btnStyle">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">50.0dp</item>
        <item name="android:background">#AAAAAA</item>
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#FFFFFF</item>
	</style>
		<style name="btnDeleteStyle">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">40.0dp</item>
        <item name="android:background">#939393</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#FFFFFF</item>
	</style>
	<style name="lista">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
    <item name="android:divider">#FFF46F</item>
    <item name="android:dividerHeight">2dp</item>       
    <item name="android:layout_marginLeft">12dp</item>
    <item name="android:layout_marginRight">12dp</item>
    <item name="android:background">#333333</item>
	</style>
	 <style name="lista.TituloItem">
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_width">fill_parent</item>
    	<item name="android:text">TextView</item>
    	<item name="android:gravity">center_vertical</item>
    	<item name="android:textColor">#555555</item>
    	<item name="android:textStyle">bold</item>
    	<item name="android:paddingTop">15.0dp</item>
    	<item name="android:textSize">15.0dp</item>
	</style>
	 <style name="lista.item">
		<item name="android:layout_height">wrap_content</item>
    	<item name="android:text">TextView</item>
    	<item name="android:gravity">left</item>
    	<item name="android:textColor">#555555</item>
    	<item name="android:paddingTop">15.0dp</item>
    	<item name="android:textSize">15.0dp</item>
	</style>
	<style name="linearLista">
		<item name="android:orientation">horizontal</item>
		<item name="android:layout_width">match_parent</item>    	
    	<item name="android:layout_height">35.0dp</item>
    	<item name="android:layout_marginLeft">10.0dp</item>
    	<item name="android:layout_marginRight">10.0dp</item>
    </style>
    <style name="headerLeft">
		<item name="android:gravity">left</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_marginTop">5.0dp</item>
		<item name="android:layout_marginBottom">5.0dp</item>
		<item name="android:layout_marginRight">5.0dp</item>
		<item name="android:layout_marginLeft">8.0dp</item>
		<item name="android:textColor">#848484</item>
		<!--<item name="android:fontFamily">sans-serif-condensed</item>-->
		<item name="android:textSize">16sp</item>
	</style>
		<style name="headerRight">
		<item name="android:gravity">right</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_marginTop">5.0dp</item>
		<item name="android:layout_marginBottom">5.0dp</item>
		<item name="android:layout_marginLeft">20.0dp</item>
		<item name="android:textColor">#000000</item>
		<!--<item name="android:fontFamily">sans-serif-condensed</item>-->
		<item name="android:textSize">16sp</item>
	</style>
	<style name="editText">
		<item name="android:layout_width">fill_parent</item>
    	<item name="android:layout_marginTop">-10dp</item>
    	<item name="android:textColor">#000000</item>
    	<!--<item name="android:fontFamily">sans-serif-condensed</item>-->
    	<item name="android:textSize">16sp</item>
	</style>
	<style name="inserirApt">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginTop">5.0dp</item>
		<item name="android:layout_marginLeft">5.0dp</item>
        <item name="android:layout_marginRight">5.0dp</item>
        <item name="android:layout_marginBottom">5.0dp</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="cabecalhoInformacoes">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:gravity">center</item>
        <item name="android:background">#222222</item>
		<!---<item name="android:background">@drawable/rounded_border</item>-->
	</style>
  <style name="cabecalhoGrid">
    <item name="android:orientation">horizontal</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">30.0dp</item>
    <item name="android:layout_marginLeft">10.0dp</item>
    <item name="android:layout_marginRight">10.0dp</item>
    <item name="android:background">#ff143613</item>
  </style>
  <style name="cabecalhoGrid.Text">
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">center_horizontal</item>
    <item name="android:paddingTop">7.0dp</item>
    <item name="android:textColor">#ffffffff</item>
  </style>
  <style name="titulo">
    <item name="android:textColor">#c5b358</item>
    <item name="android:textSize">20sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:fontFamily">sans-serif-condensed</item>
  </style>
  <style name="descricao">
    <item name="android:textStyle">bold</item>>
    <item name="android:textColor">#cccccc</item>
    <item name="android:textSize">13sp</item>
    <item name="android:fontFamily">sans-serif-light</item>
  </style>
  <style name="Divider">
    <item name="android:layout_width">1dip</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:background">#333333</item>
  </style>
  <style name="Divider_invisible">
    <item name="android:layout_width">1dip</item>
    <item name="android:layout_height">match_parent</item>
  </style>
</resources>

